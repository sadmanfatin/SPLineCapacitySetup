  <SQLQuery>
    <![CDATA[SELECT CH.MONTH_ID, 
       CH.MONTH_NAME, 
       CH.YEAR, 
       CH.ORG_ID, 
       CH.WEEKLY_HOLIDAY,
       CH.MONTH_NUMBER,
       CH.MONTH_SERIAL,
       extract (day from  last_day(to_date(CH.MONTH_NAME||'/'|| CH.YEAR , 'Month/yyyy' )) ) number_of_days,
       
       (select count ( *)  from SP_CALENDAR_L CL where CL.month_id  =  CH.month_id and  CL.WORKING_STATUS = 'Workingday')  Workingday_counT,
       
       (select count ( *)  from SP_CALENDAR_L CL where CL.month_id  =  CH.month_id and  CL.WORKING_STATUS = 'Holiday')  holiday_counT
       
FROM SP_CALENDAR_H CH]]>
  </SQLQuery>
